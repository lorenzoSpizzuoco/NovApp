<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.NovApp2.Slide">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
    </style>

    <style name="CustomSearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="android:textColor">@color/md_theme_light_primary</item>
        <item name="android:textColorHint">@color/md_theme_light_tertiary</item>
        <item name="android:inputType">textCapSentences</item>
        <item name="android:background">@drawable/searchview_background</item>
    </style>

    <style name="Theme" parent="AppTheme">
    </style>

    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

</resources>